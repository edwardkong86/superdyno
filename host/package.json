{
  "name": "superdyno",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clear": "rm -rf node_modules && rm -rf package-lock.json && rm -rf yarn.lock && yarn",
    "android": "react-native run-android --no-packager",
    "ios": "react-native run-ios --no-packager",
    "start": "react-native webpack-start",
    "bundle": "react-native webpack-bundle --platform ios --entry-file index.js --bundle-output ./dist/index.bundle --dev=false",
    "webpack": "PLATFORM=ios webpack -c webpack.config.js",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-reanimated": "^2.8.0",
    "react-native-safe-area-context": "^4.2.5",
    "react-native-screens": "^3.13.1",
    "react-native-svg": "^12.3.0",
    "redux-micro-frontend": "^1.3.0",
    "superdyno-shared": "file:../shared"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@callstack/repack": "^2.5.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@svgr/webpack": "^6.2.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.5",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-native-codegen": "^0.69.1",
    "react-test-renderer": "17.0.2",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "jest": {
    "preset": "react-native"
  }

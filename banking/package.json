{
  "name": "banking",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clear:pod": "cd ios && pod deintegrate && pod setup && pod install",
    "android": "react-native run-android --no-packager",
    "ios": "react-native run-ios --no-packager",
    "start:metro": "react-native webpack-start",
    "start": "react-native webpack-start --port 9091",
    "bundle": "react-native webpack-bundle --platform ios --entry-file index.js --bundle-output ./dist/index.bundle --dev=false",
    "webpack": "PLATFORM=ios webpack -c webpack.config.js",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-reanimated": "^2.8.0",
    "redux-micro-frontend": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@callstack/repack": "^2.5.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.5",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "jest": {
    "preset": "react-native"
  }
